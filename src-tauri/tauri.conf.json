{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kekris",
  "version": "0.0.0",
  "identifier": "com.kinire.kekris",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:4321/",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "kekris",
        "width": 800,
        "height": 600,
        "focus": false,
        "fullscreen": true,
        "minimizable": false
      },
      {
        "title": "game",
        "fullscreen": true,
        "minimizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
